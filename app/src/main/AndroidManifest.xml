<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="inas.anisha.srl_app">

    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name="inas.anisha.srl_app.SkripsiApplication"
        android:theme="@style/AppTheme">
        <receiver android:name="inas.anisha.srl_app.ui.common.AlarmReceiver" />
        <activity
            android:name="inas.anisha.srl_app.ui.splashscreen.SplashScreenActivity"
            android:label="@string/app_name"
            android:launchMode="singleTop">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="inas.anisha.srl_app.ui.main.MainActivity"
            android:launchMode="singleTop" />
        <activity android:name="inas.anisha.srl_app.ui.onboarding.OnboardingActivity" />
        <activity android:name="inas.anisha.srl_app.ui.signup.SignUpActivity" />
        <activity android:name="inas.anisha.srl_app.ui.kelolapembelajaran.KelolaPembelajaranIntroActivity" />
        <activity android:name="inas.anisha.srl_app.ui.kelolapembelajaran.KelolaPembelajaranActivity" />
        <activity android:name="inas.anisha.srl_app.ui.main.target.TargetPendukungDetailActivity" />
        <activity
            android:name="inas.anisha.srl_app.ui.main.schedule.schedule.ScheduleDetailActivity"
            android:parentActivityName="inas.anisha.srl_app.ui.main.MainActivity" />
        <activity android:name="inas.anisha.srl_app.ui.main.schedule.school.SchoolClassDetailActivity" />
        <activity android:name="inas.anisha.srl_app.ui.evaluation.EvaluationReportIntroActivity" />
        <activity android:name="inas.anisha.srl_app.ui.evaluation.EvaluationReportActivity" />
        <activity android:name="inas.anisha.srl_app.ui.updatetarget.StartNewCycleActivity" />

        <meta-data
            android:name="firebase_crashlytics_collection_enabled"
            android:value="false" />
    </application>
</manifest>
