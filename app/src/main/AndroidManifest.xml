<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="inas.anisha.skripsi_app">

    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".SkripsiApplication"
        android:theme="@style/AppTheme">
        <receiver android:name=".ui.common.AlarmReceiver" />
        <activity
            android:name=".ui.splashscreen.SplashScreenActivity"
            android:launchMode="singleTop">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.main.MainActivity"
            android:launchMode="singleTop" />
        <activity android:name=".ui.onboarding.OnboardingActivity" />
        <activity android:name=".ui.signup.SignUpActivity" />
        <activity android:name=".ui.kelolapembelajaran.KelolaPembelajaranIntroActivity" />
        <activity android:name=".ui.kelolapembelajaran.KelolaPembelajaranActivity" />
        <activity android:name=".ui.main.target.TargetPendukungDetailActivity" />
        <activity
            android:name=".ui.main.schedule.schedule.ScheduleDetailActivity"
            android:parentActivityName=".ui.main.MainActivity" />
        <activity android:name=".ui.main.schedule.school.SchoolClassDetailActivity" />
        <activity android:name=".ui.evaluation.EvaluationReportIntroActivity" />
        <activity android:name=".ui.evaluation.EvaluationReportActivity" />
        <activity android:name=".ui.updatetarget.StartNewCycleActivity" />
    </application>
</manifest>
